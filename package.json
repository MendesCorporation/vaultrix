{
  "name": "vaultrix",
  "version": "0.0.1",
  "description": "Secure Infrastructure & Credential Management Platform",
  "author": "Vaultrix Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/vaultrix.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/vaultrix/issues"
  },
  "homepage": "https://github.com/yourusername/vaultrix#readme",
  "keywords": [
    "infrastructure-management",
    "credential-management",
    "monitoring",
    "security",
    "encryption",
    "docker",
    "ssh",
    "devops",
    "self-hosted"
  ],
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "node scripts/seed.js",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "docker:build": "docker build -t vaultrix:local .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:dev": "docker-compose -f docker-compose-dev.yml up -d",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@otplib/preset-default": "^12.0.1",
    "@prisma/client": "^5.22.0",
    "@tanstack/react-query": "^5.62.8",
    "@tanstack/react-table": "^8.20.6",
    "@types/ssh2": "^1.15.5",
    "argon2": "^0.41.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.468.0",
    "next": "14.2.35",
    "next-auth": "5.0.0-beta.30",
    "nodemailer": "^7.0.12",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "server-only": "^0.0.1",
    "ssh2": "^1.17.0",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "@types/node": "^22.10.2",
    "@types/nodemailer": "^7.0.5",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-next": "^16.1.5",
    "postcss": "^8.4.49",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
